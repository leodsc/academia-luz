#include <iostream>

class Stack {
  private:
    LinkedList *linkedList = new LinkedList();
    int value;
    int size;
    Node *head;
  public:
    Stack() {
      this->size = 0;
    };

    Node *top() {
      return this->linkedList->getHead()
    };

    void push(int value) {
      this->linkedList->push(value);
    };

    int pop() {
      this->linkedList->pop();
    }

    void print() {
      if (this->size > 0) {
        Node *node = this->head;
        cout << "[ ";
        for (int i = 0; i < this->size; i++) {
          if (i != this->size-1) {
            cout << node->getValue() << ", ";
          } else {
            cout << node->getValue();
          }
          node = node->getNext();
        }
        cout << " ]" << endl;
      } else {
        cout << "Stack is empty" << endl;
      }
    };
};
